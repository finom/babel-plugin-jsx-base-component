{
	"name": "babel-plugin-jsx-conditional-component",
	"version": "0.1.5",
	"description": "Neater control exists component or component type for jsx",
	"scripts": {
		"lint": "eslint src spec *.js --ignore-pattern '!*.js'",
		"test": "mocha spec/*.js",
